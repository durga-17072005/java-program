import java.util.Scanner;

public class SumWithLeadingZeros {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();

        if (n >= 1 && n <= 50) {
            int sum = n * (n + 1) / 2;
            System.out.printf("The sum of numbers from 1 to %d : %04d\n", n, sum);
        } else {
            System.out.println("Enter a Valid Input!!!!!!!!!!!!!!!!!!");
        }
    }
}
